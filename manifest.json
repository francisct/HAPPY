{
	"name" : "Happy",
	"description" : "Browser happy",
	"version" : "1.0",
	"permissions" : [
		"background", "downloads", "tabs", "storage", "<all_urls>"
	],
	"browser_action" : {
		"default_title" : "Happy mode",
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"background" : {
		"scripts" : ["thirdParty/jquery-2.2.3.min.js", "background.js"],
		"pages" : ["popup.html"]
	},
	"manifest_version" : 2
}
